//-------------- Find Logged On User For Program Use
OPEN USERX
CLEAR USERX
MOVE USER_NUMB TO USERX.RECNUM
FIND EQ USERX BY RECNUM
MOVE USERX.USER_NAME TO USER$
MOVE USERX.ACCS_LEVL TO USER$_ACCESS
//----------------------------------------------------
//DIRECT_OUTPUT "lpt1:"
DIRECT_OUTPUT "warrant.txt"

PAGE SUMMOPT
  MOVE 'W' TO SUMMOPT.1
  ACCEPT SUMMOPT.1                {CAPSLOCK,CHECK='WS'}
  IF SUMMOPT.1 EQ 'S' CHAIN 'SUMMONS'

PAGE SELECT
MOVE 'N' TO SELECT.1
ACCEPT SELECT.1                        {CAPSLOCK,CHECK='YN '}
IF SELECT.1 EQ 'Y' CHAIN 'WARRANT3'
MOVE 'N' TO SELECT.2
ACCEPT SELECT.2                        {CAPSLOCK,CHECK='YN '}
ACCEPT SELECT.3                        {CAPSLOCK,CHECK='H '}
ACCEPT SELECT.4                        {CAPSLOCK,CHECK='L '}
MOVE 'N' TO SELECT.5
ACCEPT SELECT.5                        {CAPSLOCK,CHECK='YN '}
Move "N" to select.6
ACCEPT SELECT.6                        {CAPSLOCK,CHECK='YN '}



indicate AGGR as select.6 eq 'Y'
INDICATE FILE_DT AS SELECT.5 EQ 'Y'
MOVE 59 TO PAGEEND
MOVE 60 TO PAGEFEED
//mOVE 58 TO PAGEEND
//MOVE 59 TO PAGEFEED



INDICATE LEGAL FALSE
IF SELECT.4 IN 'Ll' BEGIN
    INDICATE LEGAL TRUE
    MOVE 84 TO PAGEEND
    MOVE 84 TO PAGEFEED
END

INDICATE MISINFOS AS SELECT.2 EQ 'Y'

IF SELECT.3 EQ 'H' MOVE -1 TO PAGEFEED

STRING YR 4 SHORT_YR 2
SYSDATE4 DATE_SCRN.1
RIGHT DATE_SCRN.1 TO YR 4
DATE AGG_DATE

CLEAR SETUP
MOVE YR TO SETUP.YEAR
FIND EQ SETUP BY INDEX.1
[FINDERR] GOSUB NOSETUP
IF SETUP.PROSNAME LE '' GOSUB NOSETUP




  TRIM SETUP.PROSNAME TO PROSECUTOR
  MOVE PROSECUTOR TO PROS
  APPEND PROSECUTOR ","
  MOVE SETUP.FBOND TO FBOND
  MOVE SETUP.MBOND TO MBOND
  MOVE SETUP.COURT_COST TO COSTS
  MOVE SETUP.FEL_COURT TO FCOURT
  MOVE 'District' TO MCOURT
  INDICATE WBOND AS SETUP.FBOND GT 0

//  MOVE "greater than $500.00 and less than $2500.00 making this a Class C felony." TO CLASSC
//  MOVE "           $2500.00 or greater making this a Class B felony.             " TO CLASSB
  MOVE "greater than $5000 and less than $25,000 making this a Class C felony." TO CLASSC
  MOVE "         $25,000.00 or greater making this a Class B felony.             " TO CLASSB
  MOVE "greater than $1000.00 and less than $5000.01 making this a Class D felony." TO CLASSD

  SYSDATE4 TODAY       
  MOVE TODAY TO PRINT_DATE 


  LEFT DATE_SCRN.1 TO MONTH 2 
  GOSUB FIND_MONTH
  MOVE MONTH_NAME TO THIS_MONTH

  MID DATE_SCRN.1 TO THIS_DAY 2 4 
  RIGHT DATE_SCRN.1 TO THIS_YEAR 4


[FILE_DT] BEGIN
    TRIM THIS_MONTH TO FILEDATE
    APPEND FILEDATE ' ' THIS_DAY
END
[~ WBOND] BEGIN
    PAGE SELECT2
    ACCEPT SELECT2.1                   {CAPSLOCK,CHECK='YN '}
    INDICATE WBOND AS SELECT2.1 EQ 'Y'
END


REPORT OFFENSE BY INDEX.7 BREAK OFFENSE.IDNO

    IF OFFENSE.CKDT GT TODAY BEGIN
         MOVE OFFENSE.IDNO   TO BADDT.1
         MOVE OFFENSE.CTRLNO TO BADDT.2
         MOVE OFFENSE.CKDT   TO BADDT.3
         MOVE DATE_SCRN.1    TO BADDT.4
         INKEY PAUSE
         ABORT    
    END

         INDICATE SELECT AS OFFENSE.ACTIVE EQ ''
[~ SELECT] GOTO END.OF.REPORT
[SELECT] INDICATE SELECT AS OFFENSE.LETDT GT ''
[SELECT] IF OFFENSE.OFFEN EQ "H" BEGIN
			INDICATE SELECT AS OFFENSE.CKDT GE (TODAY - 1095)
			END
		 Else IF OFFENSE.OFFEN EQ "M" BEGIN
			INDICATE SELECT AS OFFENSE.CKDT GE (TODAY - 365)
		 END
		 Else BEGIN
			INDICATE SELECT FALSE
		 END		
[SELECT] INDICATE SELECT AS OFFENSE.LETDT LE (TODAY - 30)
[SELECT] INDICATE SELECT AS OFFENSE.WARR_DATE EQ ''
[SELECT] IF AGG_DATE GT 0 IF OFFENSE.CKDT GT AGG_DATE BEGIN
           IF OFFENSE.IDNO EQ HOLDID GOSUB NEW_WARRANT
         END
[SELECT] IF COUNT1 GE 50 IF OFFENSE.IDNO EQ HOLDID GOSUB NEW_WARRANT
[SELECT] RELATE OFFENSE

SECTION SUBHEADER1 //------------------- Begin first warrant ------------------
        CALC (OFFENSE.CKDT + 90) TO AGG_DATE
        MOVE 0 TO COUNT1
        Move OFFENSE.IDNO TO HOLDID
        IF DEFENDNT.LIC NE '' BEGIN
           MOVE 'D.L. No ' TO DL
           APPEND DL DEFENDNT.LIC
           APPEND DL ','
        END
        ELSE MOVE '' TO DL
        APPEND DL DEFENDNT.OTHER
        POS "," IN DEFENDNT.NAME TO WHERE 
        LEFT DEFENDNT.NAME TO LNAME (WHERE-1) 
        RIGHT DEFENDNT.NAME TO FNAME (30-WHERE) 
        TRIM FNAME TO FNAME 
        APPEND FNAME " " LNAME 
        GOTOXY 12 12
        SHOW 'PROCESSING WARRANT FOR ' FNAME '                         '
        [WBOND] PRINT 'BOND $' TO SUBTOTAL1.1
        [~ WBOND] BLANKFORM SUBTOTAL1.1 THRU SUBTOTAL1.2
        PRINT COUNT 	     TO SUBTOTAL1.4
        PRINT FNAME          TO SUBTOTAL1.7
         print fname         to PG_2.1
        PRINT DEFENDNT.ADDR  TO SUBTOTAL1.8
        PRINT DEFENDNT.ADDR2 TO SUBTOTAL1.9
        PRINT DEFENDNT.CITY  TO SUBTOTAL1.10 
        PRINT DEFENDNT.ST    TO SUBTOTAL1.11
        PRINT DL             TO SUBTOTAL1.12
        TRIM JUDGE.NAME  TO THE_JUDGE
        APPEND THE_JUDGE ","
        TRIM JUDGE.CITY  TO CITY
        APPEND CITY ","
        PRINT CITY           TO SUBTOTAL1.14
        TRIM JUDGE.COUNT TO COUNT
        //APPEND COUNT ","
        MOVE JUDGE.NAME  TO JUDGE_NAME
        MOVE JUDGE.CITY  TO JUDGE_CITY
        MOVE JUDGE.COUNT TO JUDGE_COUNT
        INDICATE FELONY FALSE
		INDICATE OLDFEL FALSE
        INDICATE MISD FALSE
        BLANKFORM WITNESSES

SECTION BODY  //--------------- List bad checks, indicate felony or misd. -----
	    PRINT OFFENSE.CKAMT    TO BODY.1
		If OFFENSE.CKDT LT "07/27/2011" CALC (OLDAGG + BODY.1) TO OLDAGG		
        CALC (COUNT1 * 9)      TO WINDOWINDEX
        PRINT OFFENSE.CKNO     TO CHECK_LINES.1&
        PRINT OFFENSE.CKAMT    TO CHECK_LINES.2&
        PRINT OFFENSE.CKDT     TO CHECK_LINES.3&
        PRINT OFFENSE.BANK     TO CHECK_LINES.4&
        PRINT OFFENSE.NAME     TO CHECK_LINES.5&
        PRINT OFFENSE.MFEE     TO CHECK_LINES.6&
        PRINT OFFENSE.PFEE     TO CHECK_LINES.7&
		If OFFENSE.CKDT LT "07/27/2011" BEGIN
		   IF OFFENSE.CKAMT LT 500.01 BEGIN
		     PRINT "M" TO CHECK_LINES.8&
		     PRINT "U" TO CHECK_LINES.9&
		   END 	 
		   ELSE IF OFFENSE.CKAMT LT 2500.00 BEGIN
		     PRINT "F" TO CHECK_LINES.8&
		     PRINT "C" TO CHECK_LINES.9&
		   END 	 
		   ELSE BEGIN	 
		     PRINT "F" TO CHECK_LINES.8&
		     PRINT "B" TO CHECK_LINES.9&
           END
		END   
		ELSE If OFFENSE.CKDT GE "07/27/2011" BEGIN
		   IF OFFENSE.CKAMT LT 1000.01 BEGIN
		     PRINT "M" TO CHECK_LINES.8&
		     PRINT "A" TO CHECK_LINES.9&
		   END 	 
		   ELSE IF OFFENSE.CKAMT LT 5000.01 BEGIN
		     PRINT "F" TO CHECK_LINES.8&
		     PRINT "D" TO CHECK_LINES.9&
		   END 	 
		   ELSE IF OFFENSE.CKAMT LT 25000.00 BEGIN	 
		     PRINT "F" TO CHECK_LINES.8&
		     PRINT "C" TO CHECK_LINES.9&
		   END 	 
		   ELSE BEGIN	 
		     PRINT "F" TO CHECK_LINES.8&
		     PRINT "B" TO CHECK_LINES.9&
           END
		end   
        IF COUNT1 LE 2 BEGIN
           CALC (COUNT1 * 2)   TO WINDOWINDEX
           PRINT OFFENSE.TAKEN TO WITNESSES.1& 
           PRINT OFFENSE.NAME  TO WITNESSES.2&
        END
        INCREMENT COUNT1
        Reread Offense
        MOVE TODAY TO OFFENSE.WARR_DATE   //----- Fill in Offense.Warr_Date----
        SAVERECORD OFFENSE
        Unlock
        MOVE OFFENSE.CKDT TO PRINT_DATE 
        LEFT PRINT_DATE TO MONTH 2 
        GOSUB FIND_MONTH 
        MID PRINT_DATE TO DAY 2 4 
        RIGHT PRINT_DATE TO YEAR 4
//        IF OFFENSE.OFFEN EQ 'H' INDICATE FELONY TRUE
[~ FELONY] BEGIN
             IF OFFENSE.OFFEN EQ 'H' BEGIN
				INDICATE FELONY TRUE
				IF OFFENSE.CKDT LT "07/27/2011" INDICATE OLDFEL TRUE
			 END
             ELSE IF OFFENSE.OFFEN EQ 'M' BEGIN
                   INDICATE FELONY FALSE
                   INDICATE MISD TRUE
             END
             ELSE BEGIN
                       INDICATE FELONY FALSE
             [~ MISD]  INDICATE MISD FALSE
             END
           END

SECTION SUBTOTAL1       //--------------- Print end of warrant ----------------
